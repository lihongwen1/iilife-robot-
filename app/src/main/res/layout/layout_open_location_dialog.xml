<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/qr_code_bg">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_79">

        <TextView
            style="@style/Text_16sp_33_center"
            android:id="@+id/tv_title"
            android:text="@string/third_aty_open_location"
            android:layout_centerInParent="true"
            android:layout_marginTop="@dimen/dp_23" />
    </RelativeLayout>

    <View
        style="@style/View_Line_Style"
        android:layout_marginStart="@dimen/dp_1"
        android:layout_marginEnd="@dimen/dp_1" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_61"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_cancel"
            style="@style/Text_dialog_cancel" />

        <View
            android:layout_width="@dimen/dp_1"
            android:layout_height="match_parent"
            android:background="@color/color_e2"
            android:layout_marginBottom="@dimen/dp_2" />

        <TextView
            android:id="@+id/tv_confirm"
            style="@style/Text_dialog_confirm" />
    </LinearLayout>
</LinearLayout>