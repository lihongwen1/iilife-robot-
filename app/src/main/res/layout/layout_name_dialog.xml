<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/qr_code_bg">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="@dimen/dp_92">        <TextView            style="@style/Text_16sp_33"            android:id="@+id/tv_title"            android:text="@string/setting_aty_devName"            android:layout_centerHorizontal="true"            android:layout_marginTop="@dimen/dp_20" />        <EditText            style="@style/EditText_14sp_ac"            android:id="@+id/et_name"            android:layout_width="match_parent"            android:hint="@string/setting_aty_hit"            android:layout_centerHorizontal="true"            android:inputType="textPersonName"            android:gravity="center"            android:layout_below="@+id/tv_title"            android:layout_marginTop="@dimen/dp_10" />    </RelativeLayout>    <View        style="@style/View_Line_Style"        android:layout_marginStart="@dimen/dp_1"        android:layout_marginEnd="@dimen/dp_1"/>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="@dimen/dp_48"        android:orientation="horizontal">        <TextView            android:id="@+id/tv_cancel"            style="@style/Text_dialog_cancel"/>        <View            android:layout_width="@dimen/dp_1"            android:layout_height="match_parent"            android:background="@color/color_e2"            android:layout_marginBottom="@dimen/dp_2"/>        <TextView            android:id="@+id/tv_confirm"            style="@style/Text_dialog_confirm"/>    </LinearLayout></LinearLayout>