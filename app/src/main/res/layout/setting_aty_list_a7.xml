<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/rl_clock"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/clock_aty_appoint"/>
        <ImageView
            style="@style/Image_Style_Forward"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"/>
    <RelativeLayout
        android:id="@+id/rl_record"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:visibility="gone">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_clean_record"/>
        <ImageView
            style="@style/Image_Style_Forward"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/rl_consume"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_consume_detail"/>
        <ImageView
            style="@style/Image_Style_Forward"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/rl_water"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:background="@drawable/setting_aty_water_bg"
        android:visibility="gone">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:id="@+id/tv_control"
            android:text="@string/setting_aty_water_control"/>
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_12sp_ac"
            android:id="@+id/tv_water"
            android:layout_toEndOf="@+id/tv_control"
            android:layout_toStartOf="@+id/image_down_1"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:gravity="end"
            android:text="@string/setting_aty_standard"/>
        <ImageView
            android:id="@+id/image_down_1"
            style="@style/Image_Style_Down"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"/>
    <LinearLayout
        android:id="@+id/ll_water"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone">
        <RelativeLayout
            android:id="@+id/rl_soft"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@color/color_f6">
            <com.ilife.iliferobot_cn.view.LightTextView
                style="@style/Text_12sp_ac"
                android:id="@+id/tv_soft"
                android:text="@string/setting_aty_soft"
                android:layout_marginStart="@dimen/dp_30"
                android:textColor="@drawable/tv_water_color"/>
            <ImageView
                android:id="@+id/image_soft"
                android:layout_width="@dimen/dp_42"
                android:layout_height="@dimen/dp_14"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_30"
                android:background="@drawable/image_soft_bg"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/color_e2"
            android:layout_marginStart="@dimen/dp_30"
            android:layout_marginEnd="@dimen/dp_30"/>
        <RelativeLayout
            android:id="@+id/rl_standard"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@color/color_f6">
            <com.ilife.iliferobot_cn.view.LightTextView
                style="@style/Text_12sp_ac"
                android:id="@+id/tv_standard"
                android:text="@string/setting_aty_standard"
                android:layout_marginStart="@dimen/dp_30"
                android:textColor="@drawable/tv_water_color"/>
            <ImageView
                android:id="@+id/image_standard"
                android:layout_width="@dimen/dp_42"
                android:layout_height="@dimen/dp_14"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_30"
                android:background="@drawable/image_standard_bg"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/color_e2"
            android:layout_marginStart="@dimen/dp_30"
            android:layout_marginEnd="@dimen/dp_30"/>
        <RelativeLayout
            android:id="@+id/rl_strong"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@color/color_f6">
            <com.ilife.iliferobot_cn.view.LightTextView
                style="@style/Text_12sp_ac"
                android:id="@+id/tv_strong"
                android:text="@string/setting_aty_strong"
                android:layout_marginStart="@dimen/dp_30"
                android:textColor="@drawable/tv_water_color"/>
            <ImageView
                android:id="@+id/image_strong"
                android:layout_width="@dimen/dp_42"
                android:layout_height="@dimen/dp_14"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_30"
                android:background="@drawable/image_strong_bg"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/color_e2"/>
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/rl_mode"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <ImageView
            android:id="@+id/image_down_2"
            style="@style/Image_Style_Down"/>
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_12sp_ac"
            android:id="@+id/tv_mode"
            android:layout_toEndOf="@+id/tv_mode_"
            android:layout_toStartOf="@+id/image_down_2"
            android:text="@string/map_aty_plan_mode"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginEnd="@dimen/dp_20"
            android:gravity="end"/>
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_clean_mode1"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="@dimen/dp_30"
            android:id="@+id/tv_mode_" />
        <View style="@style/View_Line_Style"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/ll_mode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone">
        <RelativeLayout
            android:id="@+id/rl_plan"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@color/color_f6">
            <com.ilife.iliferobot_cn.view.LightTextView
                style="@style/Text_12sp_ac"
                android:id="@+id/tv_plan"
                android:text="@string/setting_aty_nav_clean"
                android:layout_marginStart="@dimen/dp_30"
                android:textColor="@drawable/tv_water_color"/>
            <ImageView
                android:id="@+id/image_plan"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_17"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_30"
                android:background="@drawable/n_btn_model_bg"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/color_e2"
            android:layout_marginStart="@dimen/dp_30"
            android:layout_marginEnd="@dimen/dp_30"/>
        <RelativeLayout
            android:id="@+id/rl_random"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60"
            android:background="@color/color_f6">
            <com.ilife.iliferobot_cn.view.LightTextView
                style="@style/Text_12sp_ac"
                android:id="@+id/tv_random"
                android:text="@string/setting_aty_random_clean"
                android:layout_marginStart="@dimen/dp_30"
                android:textColor="@drawable/tv_water_color"/>
            <ImageView
                android:id="@+id/image_random"
                android:layout_width="@dimen/dp_17"
                android:layout_height="@dimen/dp_17"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_30"
                android:background="@drawable/n_btn_model_bg"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/color_e2" />
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/rl_voice"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:visibility="gone">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_voice_mode"/>
        <ImageView
            android:id="@+id/image_voice"
            android:layout_width="@dimen/dp_49"
            android:layout_height="@dimen/dp_28"
            android:background="@drawable/image_max_bg"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_30"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"
        android:visibility="gone"/>
    <RelativeLayout
        android:id="@+id/rl_suction"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_max_suction"/>
        <ImageView
            android:id="@+id/image_max"
            android:layout_width="@dimen/dp_49"
            android:layout_height="@dimen/dp_28"
            android:background="@drawable/image_max_bg"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_30"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:background="@color/color_f6"/>
    <View style="@style/View_Line_Style"/>
    <RelativeLayout
        android:id="@+id/rl_find"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_find_robot"/>
        <ImageView
            android:id="@+id/imageView"
            android:layout_width="@dimen/dp_18"
            android:layout_height="@dimen/dp_18"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_19"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="@dimen/dp_30"
            android:src="@drawable/n_icon_progress_yellow"
            android:visibility="gone"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"/>
    <RelativeLayout
        android:id="@+id/rl_facReset"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <com.ilife.iliferobot_cn.view.LightTextView
            style="@style/Text_16sp_33_center"
            android:text="@string/setting_aty_factory_reset"/>
        <ImageView
            style="@style/Image_Style_Forward"/>
    </RelativeLayout>
    <View style="@style/View_Line_Style"/>
</LinearLayout>